{
    "version": 2,
    "builds": [
      {
        "src": "streetgard_backend/streetgard_backend/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
      },
      {
        "src": "streetgard_backend/build_files.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "static"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "streetgard_backend/static/$1"
      },
      {
        "src": "/media/(.*)",
        "dest": "streetgard_backend/media/$1"
      },
      {
        "src": "/(.*)",
        "dest": "streetgard_backend/streetgard_backend/wsgi.py"
      }
    ]
  }